<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:height="50dp"
        android:top="10dp"
        android:left="20dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/mainOrange"/>
            <stroke
                android:color="@color/DarkerOrange"
                android:width="5dp"/>
        </shape>
    </item>
    <item
        android:height="75dp"
        android:width="75dp"
        >
        <shape android:shape="oval">
            <solid android:color="@color/mainOrange"/>
            <stroke
                android:color="@color/DarkerOrange"
                android:width="5dp"/>
        </shape>
    </item>
</layer-list>