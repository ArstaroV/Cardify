<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:height="50dp"
        android:left="20dp"
        android:top="12dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/mainOrange" />
            <stroke
                android:width="5dp"
                android:color="@color/DarkerOrange" />
        </shape>
    </item>
    <item
        android:width="75dp"
        android:height="75dp">
        <shape android:shape="oval">
            <solid android:color="@color/mainOrange" />
            <stroke
                android:width="5dp"
                android:color="@color/DarkerOrange" />
        </shape>
    </item>
    <item
        android:width="60dp"
        android:height="60dp"
        android:drawable="@drawable/inbox"
        android:top="5dp"
        android:left="7dp"/>
</layer-list>