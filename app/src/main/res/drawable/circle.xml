<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:width="90dp"
          android:height="90dp">
        <shape android:shape="oval">
            <solid android:color="#FFA500"/>
            <stroke android:color="@color/DarkerOrange"
                    android:width="2dp"/>
            <size android:width="90dp"
                  android:height="90dp"/>
        </shape>
    </item>
    <item android:width="40dp" android:gravity="center">
        <shape android:shape="line">
            <stroke android:width="3dp" android:color="@color/DarkerOrange"/>
        </shape>
    </item>
    <item android:width="40dp" android:gravity="center">
        <rotate android:fromDegrees="90"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="90">
            <shape android:shape="line">
                <stroke android:width="3dp" android:color="@color/DarkerOrange"/>
            </shape>
        </rotate>
    </item>
</layer-list>